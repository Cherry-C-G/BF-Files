@using CourseDaoApplication.Data
@model Professor

@{
    ViewData["Title"] = "Professor Details";
}

<h2>@Model.FirstName</h2>
<h2>@Model.LastName</h2>

<dl class="row">
    <dt class="col-sm-2">Email</dt>
    <dd class="col-sm-10">@Model.Email</dd>

    <dt class="col-sm-2">Office</dt>
    <dd class="col-sm-10">@Model.Office</dd>

    <dt class="col-sm-2">Title</dt>
    <dd class="col-sm-10">@Model.Title</dd>
</dl>

<a asp-action="Index">Back to List</a>
