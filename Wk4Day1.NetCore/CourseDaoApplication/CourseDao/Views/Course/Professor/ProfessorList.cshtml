@using CourseDaoApplication.Data
@model List<Professor>
<h2>List of Professors</h2>
<table>
    <thead>
        <tr>
            <th>Name</th>
            <th>Email</th>
            <th>Office</th>
            <th>Title</th>
            <th>Number of Courses</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var professor in Model)
        {
            <tr>
                <td>@professor.FirstName @professor.LastName</td>
                <td>@professor.Email</td>
                <td>@professor.Office</td>
                <td>@professor.Title</td>
               @* <td>@professor.Courses.Count</td>*@
            </tr>
        }
    </tbody>
</table>
